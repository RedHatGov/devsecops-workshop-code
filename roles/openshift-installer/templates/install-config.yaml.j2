apiVersion: v1
baseDomain: {{ openshift_base_domain }}
compute:
- hyperthreading: Enabled
  name: worker
  platform:
    aws:
      type: '{{ cluster_worker_machine_type }}'
  replicas: 2
controlPlane:
  hyperthreading: Enabled
  name: master
  platform:
    aws:
      type: '{{ cluster_worker_machine_type }}'
  replicas: 3
metadata:
  creationTimestamp: null
  name: {{ cluster_name }}
networking:
  clusterNetwork:
  - cidr: 10.128.0.0/14
    hostPrefix: 23
  machineCIDR: 10.0.0.0/16
  networkType: OpenShiftSDN
  serviceNetwork:
  - 172.30.0.0/16
platform:
  aws:
    region: {{ aws_region }}
pullSecret: '{{ cloud_redhat_com_pull_secret }}'
sshKey: {{ cluster_key.public_key }}
